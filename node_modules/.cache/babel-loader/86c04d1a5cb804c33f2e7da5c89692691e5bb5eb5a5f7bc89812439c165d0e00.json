{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.reduce.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      customer: {\n        name: '',\n        contact: '',\n        address: ''\n      },\n      products: [],\n      newProduct: {\n        name: '',\n        qty: 0,\n        unit_price: 0\n      },\n      subtotal: 0,\n      tax: 0,\n      total: 0\n    };\n  },\n  methods: {\n    // Fetch products from the API\n    fetchProducts() {\n      axios.get('http://your-laravel-app.com/api/products').then(response => {\n        this.products = response.data;\n        this.calculateSummary();\n      }).catch(error => console.error('Error fetching products:', error));\n    },\n    // Add a new product to the inventory\n    addProduct() {\n      const productData = {\n        name: this.newProduct.name,\n        quantity: this.newProduct.quantity,\n        unit_price: this.newProduct.unit_price\n      };\n      axios.post('http://your-laravel-app.com/api/products', productData).then(response => {\n        this.products.push(response.data); // Add the new product to the table\n        this.newProduct = {\n          name: '',\n          quantity: 0,\n          unit_price: 0\n        }; // Reset the new product fields\n        this.calculateSummary(); // Recalculate the totals\n      }).catch(error => console.error('Error adding product:', error));\n    },\n    // Calculate the billing summary\n    calculateSummary() {\n      this.subtotal = this.products.reduce((sum, product) => sum + product.quantity * product.unit_price, 0);\n      this.tax = this.subtotal * 0.05;\n      this.total = this.subtotal + this.tax;\n    },\n    // Generate the bill (navigate to invoice page)\n    generateBill() {\n      this.$router.push('/invoice');\n    },\n    // Example methods for editing and deleting products\n    editProduct(id) {\n      console.log('Edit product with id:', id);\n    },\n    deleteProduct(id) {\n      console.log('Delete product with id:', id);\n    }\n  },\n  created() {\n    this.fetchProducts(); // Fetch products when the component is created\n  }\n};","map":{"version":3,"names":["axios","data","customer","name","contact","address","products","newProduct","qty","unit_price","subtotal","tax","total","methods","fetchProducts","get","then","response","calculateSummary","catch","error","console","addProduct","productData","quantity","post","push","reduce","sum","product","generateBill","$router","editProduct","id","log","deleteProduct","created"],"sources":["H:\\vue\\vue_inventory\\src\\components\\Bill.vue"],"sourcesContent":["<template>\r\n    <div class=\"billing-container\">\r\n        <header class=\"header\">\r\n            <h1>Billing System</h1>\r\n            <p>Inventory Management</p>\r\n        </header>\r\n\r\n        <!-- Customer Details Section -->\r\n        <section class=\"customer-details\">\r\n            <h2>Customer Information</h2>\r\n            <label for=\"customer-name\">Name:</label>\r\n            <input type=\"text\" id=\"customer-name\" v-model=\"customer.name\" placeholder=\"Enter Customer Name\">\r\n            \r\n            <label for=\"customer-contact\">Contact:</label>\r\n            <input type=\"text\" id=\"customer-contact\" v-model=\"customer.contact\" placeholder=\"Enter Contact Number\">\r\n\r\n            <label for=\"customer-address\">Address:</label>\r\n            <textarea id=\"customer-address\" v-model=\"customer.address\" placeholder=\"Enter Address\"></textarea>\r\n        </section>\r\n\r\n        <button @click=\"addProduct\">Add Product</button>\r\n\r\n        <!-- Product List Section -->\r\n        <section class=\"product-list\">\r\n            <h2>Products</h2>\r\n            <table id=\"product-table\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>Product Name</th>\r\n                        <th>Quantity</th>\r\n                        <th>Unit Price</th>\r\n                        <th>Total</th>\r\n                        <th>Action</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"product in products\" :key=\"product.id\">\r\n                        <td>{{ product.name }}</td>\r\n                        <td>{{ product.qty }}</td>\r\n                        <td>{{ product.price }}</td>\r\n                        <td>{{ product.qty * product.price }}</td>\r\n                        <td><button @click=\"editProduct(product.id)\">Edit</button> | <button @click=\"deleteProduct(product.id)\">Delete</button></td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </section>\r\n\r\n        <!-- Billing Summary Section -->\r\n        <section class=\"billing-summary\">\r\n            <h2>Billing Summary</h2>\r\n            <p><strong>Subtotal:</strong> $<span>{{ subtotal }}</span></p>\r\n            <p><strong>Tax (5%):</strong> $<span>{{ tax }}</span></p>\r\n            <p><strong>Total Amount:</strong> $<span>{{ total }}</span></p>\r\n\r\n            <button id=\"generate-bill\" @click=\"generateBill\">Generate Bill</button>\r\n        </section>\r\n        \r\n        <!-- Footer Section -->\r\n        <footer class=\"footer\">\r\n            <p>Thank you</p>\r\n        </footer>\r\n    </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            customer: {\r\n                name: '',\r\n                contact: '',\r\n                address: ''\r\n            },\r\n            products: [],\r\n            newProduct: {\r\n                name: '',\r\n                qty: 0,\r\n                unit_price: 0\r\n            },\r\n            subtotal: 0,\r\n            tax: 0,\r\n            total: 0\r\n        };\r\n    },\r\n    methods: {\r\n        // Fetch products from the API\r\n        fetchProducts() {\r\n            axios.get('http://your-laravel-app.com/api/products')\r\n                .then(response => {\r\n                    this.products = response.data;\r\n                    this.calculateSummary();\r\n                })\r\n                .catch(error => console.error('Error fetching products:', error));\r\n        },\r\n\r\n        // Add a new product to the inventory\r\n        addProduct() {\r\n            const productData = {\r\n                name: this.newProduct.name,\r\n                quantity: this.newProduct.quantity,\r\n                unit_price: this.newProduct.unit_price\r\n            };\r\n\r\n            axios.post('http://your-laravel-app.com/api/products', productData)\r\n                .then(response => {\r\n                    this.products.push(response.data); // Add the new product to the table\r\n                    this.newProduct = { name: '', quantity: 0, unit_price: 0 }; // Reset the new product fields\r\n                    this.calculateSummary(); // Recalculate the totals\r\n                })\r\n                .catch(error => console.error('Error adding product:', error));\r\n        },\r\n\r\n        // Calculate the billing summary\r\n        calculateSummary() {\r\n            this.subtotal = this.products.reduce((sum, product) => sum + (product.quantity * product.unit_price), 0);\r\n            this.tax = this.subtotal * 0.05;\r\n            this.total = this.subtotal + this.tax;\r\n        },\r\n\r\n        // Generate the bill (navigate to invoice page)\r\n        generateBill() {\r\n            this.$router.push('/invoice');\r\n        },\r\n\r\n        // Example methods for editing and deleting products\r\n        editProduct(id) {\r\n            console.log('Edit product with id:', id);\r\n        },\r\n        deleteProduct(id) {\r\n            console.log('Delete product with id:', id);\r\n        }\r\n    },\r\n    created() {\r\n        this.fetchProducts(); // Fetch products when the component is created\r\n    }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n/* styles.css */\r\nbody {\r\n    font-family: Arial, sans-serif;\r\n    background-color: #f4f4f9;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n.billing-container {\r\n    width: 80%;\r\n    margin: 20px auto;\r\n    background-color: #fff;\r\n    padding: 20px;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.header h1 {\r\n    text-align: center;\r\n    color: #333;\r\n}\r\n\r\n.customer-details,\r\n.product-list,\r\n.billing-summary {\r\n    margin-bottom: 20px;\r\n}\r\n\r\nh2 {\r\n    color: #4CAF50;\r\n}\r\n\r\nlabel {\r\n    font-weight: bold;\r\n    margin-top: 10px;\r\n    display: block;\r\n}\r\n\r\ninput, textarea {\r\n    width: 100%;\r\n    padding: 8px;\r\n    margin: 8px 0;\r\n    border: 1px solid #ccc;\r\n    border-radius: 4px;\r\n}\r\n\r\ntextarea {\r\n    resize: vertical;\r\n    height: 80px;\r\n}\r\n\r\ntable {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    margin-top: 20px;\r\n}\r\n\r\ntable, th, td {\r\n    border: 1px solid #ddd;\r\n}\r\n\r\nth, td {\r\n    padding: 10px;\r\n    text-align: center;\r\n}\r\n\r\nbutton {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n    padding: 10px 15px;\r\n    cursor: pointer;\r\n    border-radius: 4px;\r\n    font-size: 16px;\r\n    margin-top: 10px;\r\n}\r\n\r\nbutton:hover {\r\n    background-color: #45a049;\r\n}\r\n\r\nfooter {\r\n    text-align: center;\r\n    margin-top: 20px;\r\n    font-size: 14px;\r\n    color: #888;\r\n}\r\n</style>\r\n"],"mappings":";;;AAkEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QACNC,IAAI,EAAE,EAAE;QACRC,OAAO,EAAE,EAAE;QACXC,OAAO,EAAE;MACb,CAAC;MACDC,QAAQ,EAAE,EAAE;MACZC,UAAU,EAAE;QACRJ,IAAI,EAAE,EAAE;QACRK,GAAG,EAAE,CAAC;QACNC,UAAU,EAAE;MAChB,CAAC;MACDC,QAAQ,EAAE,CAAC;MACXC,GAAG,EAAE,CAAC;MACNC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;E