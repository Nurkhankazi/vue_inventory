\n            margin-left: 20px;\r\n            border-radius: 8px;\r\n        }\r\n\r\n        .main-content h2 {\r\n            margin-top: 0;\r\n        }\r\n\r\n        table {\r\n            width: 100%;\r\n            border-collapse: collapse;\r\n            margin-top: 20px;\r\n        }\r\n\r\n        table th, table td {\r\n            padding: 10px;\r\n            text-align: left;\r\n            border: 1px solid #ddd;\r\n        }\r\n\r\n        table th {\r\n            background-color: #f2f2f2;\r\n        }\r\n\r\n        .form-group {\r\n            margin-bottom: 15px;\r\n        }\r\n\r\n        .form-group input {\r\n            padding: 8px;\r\n            width: 100%;\r\n            margin-top: 5px;\r\n            box-sizing: border-box;\r\n        }\r\n\r\n        .button {\r\n            padding: 10px 20px;\r\n            background-color: #4CAF50;\r\n            color: white;\r\n            border: none;\r\n            cursor: pointer;\r\n            border-radius: 5px;\r\n        }\r\n\r\n        .button:hover {\r\n            background-color: #45a049;\r\n        }\r\n    </style>\r\n\r\n    \r\n\r\n    <script>\r\n        function addItem() {\r\n            var itemName = document.getElementById(\"item-name\").value;\r\n            var itemQuantity = document.getElementById(\"item-quantity\").value;\r\n\r\n            if (itemName && itemQuantity) {\r\n                var table = document.getElementById(\"inventory-table\").getElementsByTagName('tbody')[0];\r\n\r\n                var newRow = table.insertRow();\r\n                var cell1 = newRow.insertCell(0);\r\n                var cell2 = newRow.insertCell(1);\r\n                var cell3 = newRow.insertCell(2);\r\n\r\n                cell1.textContent = itemName;\r\n                cell2.textContent = itemQuantity;\r\n                cell3.innerHTML = '<button class=\"button\" onclick=\"removeItem(this)\">Remove</button>';\r\n\r\n                document.getElementById(\"item-name\").value = \"\";\r\n                document.getElementById(\"item-quantity\").value = \"\";\r\n            } else {\r\n                alert(\"Please fill in both fields.\");\r\n            }\r\n        }\r\n\r\n        function removeItem(button) {\r\n            var row = button.parentNode.parentNode;\r\n            row.parentNode.removeChild(row);\r\n        }\r\n    </script>\r\n\r\n// import ApexCharts from 'apexcharts'\r\n export default{\r\n     name:'Home',\r\n     props:{\r\n            msg:String\r\n        }\r\n }"],"mappings":"AA8IQ,SAASA,OAAOA,CAAA,EAAG;EACf,IAAIC,QAAO,GAAIC,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,KAAK;EACzD,IAAIC,YAAW,GAAIH,QAAQ,CAACC,cAAc,CAAC,eAAe,CAAC,CAACC,KAAK;EAEjE,IAAIH,QAAO,IAAKI,YAAY,EAAE;IAC1B,IAAIC,KAAI,GAAIJ,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC,CAACI,oBAAoB,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC;IAEvF,IAAIC,MAAK,GAAIF,KAAK,CAACG,SAAS,CAAC,CAAC;IAC9B,IAAIC,KAAI,GAAIF,MAAM,CAACG,UAAU,CAAC,CAAC,CAAC;IAChC,IAAIC,KAAI,GAAIJ,MAAM,CAACG,UAAU,CAAC,CAAC,CAAC;IAChC,IAAIE,KAAI,GAAIL,MAAM,CAACG,UAAU,CAAC,CAAC,CAAC;IAEhCD,KAAK,CAACI,WAAU,GAAIb,QAAQ;IAC5BW,KAAK,CAACE,WAAU,GAAIT,YAAY;IAChCQ,KAAK,CAACE,SAAQ,GAAI,mEAAmE;IAErFb,QAAQ,CAACC,cAAc,CAAC,WAAW,CAAC,CAACC,KAAI,GAAI,EAAE;IAC/CF,QAAQ,CAACC,cAAc,CAAC,eAAe,CAAC,CAACC,KAAI,GAAI,EAAE;EACvD,OAAO;IACHY,KAAK,CAAC,6BAA6B,CAAC;EACxC;AACJ;AAEA,SAASC,UAAUA,CAACC,MAAM,EAAE;EACxB,IAAIC,GAAE,GAAID,MAAM,CAACE,UAAU,CAACA,UAAU;EACtCD,GAAG,CAACC,UAAU,CAACC,WAAW,CAACF,GAAG,CAAC;AACnC","ignoreList":[]},"metadata":{},"sourceType":"script","externalDependencies":[]}